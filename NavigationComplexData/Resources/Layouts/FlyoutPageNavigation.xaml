<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:NavigationComplexData"
            xmlns:layput="clr-namespace:NavigationComplexData.Resources.Layout"
            x:Class="NavigationComplexData.Resources.Layout.FlyoutPageNavigation">
    <FlyoutPage.Flyout>
        <layput:FlyMenuPage x:Name="flyoutMenu"/>
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <local:MainPage/>
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>